<div class="container" id="topicList">
    {{#each data}}
        <div class="row posts">
            <div class="col-md-12">
                <div class="row">
                    <div class="col-md-2">
                        <p>{{User.username}}</p>
                    </div>
                    <div class="col-md-8">
                        <p>{{updatedAt}}</p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-10 col-md-offset-2 postContent">
                        <div id={{id}}></div>
                    </div>
                </div>
            </div>
        </div>
        <br>
    {{/each}}
</div>
<script src="https://code.jquery.com/jquery.js"></script>
<script src="public/assets/javascript/marked.min.js"></script>
<script>
    $(document).ready(() => {
        {{#each data}}
            var post = `{{{post}}}`
            console.log((post))
            $("#{{id}}").html(marked(post));
        {{/each}}
    });
</script>
